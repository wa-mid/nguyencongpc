.box-product-item{
    width: 628px;
    margin: 0 0 0 10px;
}

.grid-item .text,
.product-item .text{
    padding:5px;
    font-size: 12px;
    position:relative;
    background:url(/images/bg-p-item.png) no-repeat top right;
    background-color: #407ed8;
    color: #fff;
}
.product-item .text span.name{
    display:block;
    height: 40px;
    overflow: hidden;
}
.product-item .text .new-price{
    font-weight: bold;
    font-size:16px;
    display: block;
    color: #FF9800;
}
.product-item .text .sale{
    position:absolute;
    padding:5px 10px;
    background: #F9AA28;
    border-radius: 3px;
    font-weight: bold;
    bottom:10px;
    right:10px;
    height: auto;
}
.product-item a:hover .text{
    color:#dcdcdc;
}
.product-item .text .old-price{
    text-decoration: line-through;
}
.submenu.lv1 .list-product .product-item{
    width:calc(33.33% - 10px);
    float:left;
    margin:0px 5px 10px 5px;
}

#product-sale{
    width:900px;
    overflow: hidden;
    margin:0 0 0 10px;
}
.right-content .block .head,
.main-product .head,
#product-sale .head{
    border: 1px solid #FFE3B5;
    border-left-width: 5px;
    background: #FFF9EF;
    margin:0 10px 10px 0;
    position:relative;
    height:34px;
}
.main-product .head:before,
#product-sale .head:before{
    content:"";
    border:16px solid transparent;
    border-left:16px solid #FFF9EF;
    position: absolute;
    left: 100%;
    top: 0px;
    z-index: 2;
}
.main-product .head:after,
#product-sale .head:after{
    content: "";
    border: 17px solid transparent;
    border-left: 17px solid #FFE3B5;
    position: absolute;
    left: 100%;
    top: -1px;
}
.main-product .head ul,
#product-sale .head p{
    padding: 0px 10px;
    line-height: 33px;
    text-transform: uppercase;
    color:#F49B0A;
    font-weight: bold;
    float:left;
}
.main-product .head > a,
#product-sale .head a{
    float: right;
    background: #FFA207;
    font-size: 12px;
    margin: 6px 5px;
    padding: 1px 5px 1px 5px;
    border-left: 3px solid #FF7D00;
    position:relative;
    color:#ffffff;
}
.main-product .head > a:after,
#product-sale .head a:after{
    content: "";
    border: 10px solid transparent;
    border-left: 10px solid #FFA207;
    position: absolute;
    left: 100%;
    top: 0px;
    z-index: 3;
}
.box-slide{
    position:relative;
    overflow: hidden;
}
.product-detail .product-images{
    width: 850px;
    height: 600px;
}
.product-detail .product-images:before{
    animation-duration: 1s;
    animation-fill-mode: forwards;
    animation-iteration-count: infinite;
    animation-name: fbStoriesPlaceHolderShimmer;
    animation-timing-function: steps(10, end);
    background-image: linear-gradient( to right, #F5F6F7 0%, #e9ebee 20%, #F5F6F7 40%, #F5F6F7 100% );
    background-repeat: no-repeat;
    background-size: 850px 600px;
    content: ' ';
    display: block;
    height: 100%;
    position: absolute;
}
.product-images .box-slide{
    position:relative;
    overflow: hidden;
}
.box-slide .owl-carousel .owl-nav button.owl-next,
.box-slide .owl-carousel .owl-nav button.owl-prev{
    margin-top: -20px;
    position: absolute;
    top: 50%;
    padding: 20px;
    background: #f9aa28;
    font-size:40px;
    width: 40px;
    height: 40px;
    line-height: 35px;
    border-radius: 2px;
    color:#fff;
    outline:none;
    transition: all 0.3s ease-in-out;
}
.box-slide .owl-carousel .owl-nav button.owl-prev,
.box-slide .owl-carousel .owl-nav button.owl-next{
    background: rgba(255, 255, 255, 0.3);
    font-size:40px;
    width: 30px;
    height: 40px;
}

.box-slide .owl-carousel .owl-nav button.owl-next{
    right:-50px;
}
.box-slide .owl-carousel .owl-nav button.owl-prev{
    left:-50px;
}
.box-slide:hover .owl-carousel .owl-nav button.owl-next{
    right:10px;
}
.box-slide:hover .owl-carousel .owl-nav button.owl-prev{
    left:10px;
}

.box-slide .owl-carousel .owl-nav button.owl-next :after,
.box-slide .owl-carousel .owl-nav button.owl-prev :after{
    content:"";
    width:20px;
    height: 40px;
    z-index: 2;
    background: #fff;
    position:absolute;
    top:2px;
    left:17px;
}
.box-slide .owl-carousel .owl-nav button.owl-next :after,
.box-slide .owl-carousel .owl-nav button.owl-prev :after{
    background: none;
}
.box-slide .owl-carousel .owl-nav button.owl-next :after,
.box-slide .owl-carousel .owl-nav button.owl-next :after{
    left:inherit;
    right:17px;
}
.box-slide .owl-carousel .owl-nav button.owl-next  span,
.box-slide .owl-carousel .owl-nav button.owl-prev  span {
    position: absolute;
    left: 5px;
    top: -2px;
}
.box-slide .owl-carousel .owl-nav button.owl-next  span{
    left:inherit;
    right:5px;
}
.box-slide .owl-dots{
    position: absolute;
    bottom: 10px;
    left: 0;
    right: 0;
}
.box-slide .owl-theme .owl-dots .owl-dot span{
     background: #fff;
}

#product-sale .box-slide .owl-carousel .owl-nav button{
    margin-top:-50px;
}
