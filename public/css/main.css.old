    text-transform: uppercase;
    font-size: 12px;
}
#header .buid-config img{
    width: 30px;
    margin: 0 5px 0 0;
}
#header .cart .count-item{
    position: absolute;
    top: -10px;
    background: #ff0000;
    border-radius: 50%;
    padding: 0px 7px;
}

