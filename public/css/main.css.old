@font-face {
    font-family: Nunito;
    src: url(fonts/Nunito-Regular.ttf);
}
::-webkit-input-placeholder { /* Chrome/Opera/Safari */
    color: #fff;
}
::-moz-placeholder { /* Firefox 19+ */
    color: #fff;
}
:-ms-input-placeholder { /* IE 10+ */
    color: #fff;
}
:-moz-placeholder { /* Firefox 18- */
    color: #fff;
}
.sprite{
    background-image: url(../images/sprite.png?v=1.2);
    background-repeat: no-repeat;
}
body{
    font-size:14px;
    font-family: 'Roboto', sans-serif;
    background: #F7F7F7;
}
a{
    color:#333;
}
a:hover{
    text-decoration: none;
}
iframe,
img{
    max-width:100%;
    height: auto;
}
p,ul,li,h1,h2,h3,ol,form{
    margin:0;
    padding:0;
}
.left{
    float:left;
}
.right{
    float:right;
}
.clearfix{
    clear: both;
}
ul li{
    list-style:none;
}
.container{
    width: 1200px;
    max-width: 100%;
    padding: 0 10px;
    margin-right: auto;
    margin-left: auto;
    overflow: hidden;
}
#content > .container{
    background: #fff;
}
#header{
    background: rgb(15,91,154);
    background: linear-gradient(90deg, rgba(15,91,154,1) 0%, rgba(76,157,220,1) 100%);
    color:#fff;
}
#header a{
    color:#fff;
}

.top-header {
    background: rgb(29,101,160);
    background: linear-gradient(90deg, rgba(29,101,160,1) 0%, rgba(86,162,222,1) 100%);
    height: 35px;
    line-height: 35px;
    color:#fff;
}

.top-header .header-item{
    float:left;
    margin:0 0 0 60px;
    font-weight: bold;
}

.top-header .header-item.hotline{
    font-weight:bold;
    font-size:16px;
    color: #FFB233;
}
.top-header .header-item.hotline .fa-phone,
.top-header .header-item.support .fa-support{
    background: #fff;
    color: #418ac6;
    border-radius: 50%;
    padding: 4px;
}
.top-header .header-item.hotline .fa-phone{
    padding:4px 6px;
    color:#FFB233;
}
.top-header .header-item a{
    padding:10px 20px;
    color:#fff;
}

.top-header .header-item .menu ul li{
    float:left;
    font-weight: normal;
}
.top-header .header-item .menu ul li a:hover{
    background:#68a8da;
}
#main-logo{
}
#main-logo img{
    width: 120px;
    margin: 0 0 0 0;
}
.mid-header{
    padding:10px 0;
    transition: all 0.2s ease;
    background: rgb(15,91,154);
    background: linear-gradient(90deg, rgba(15,91,154,1) 0%, rgba(76,157,220,1) 100%);
}
.fixtop .mid-header{
    position:fixed;
    top:0;
    left:0;
    right:0;
    z-index: 6;
}
.box-search .text{
    margin: 0 0 5px 0;
}
.mid-header .box-search{
    width:640px;
    margin:0 0 0 20px;
}
#main-search{
    margin:0 0 10px 0;
}
#main-search .text{

}
#main-search .input-search{
    width:100%;
    position: relative;
}
#main-search .input-search .icon-search{
    position:absolute;
    top: 3px;
    right: 5px;
    width: 55px;
    height: 30px;
    background: #FF9F00;
    text-align: center;
    padding: 5px 0;
    cursor: pointer;

}
#main-search .input-search input{
    width: 100%;
    outline: none;
    height: 35px;
    border: 1px solid #4E98D6;
    border-radius: 2px;
    background-color: rgba(255,255,255,0.12);
    box-shadow: 0 2px 4px 0 rgba(0,0,0,0.04);
    color:#fff;
    padding:5px;
}

#header .tags-suggest a{
    display: inline-block;
    padding: 2px 5px 0 5px;
    border-right: 1px solid #dcdcdc;
    color: #ffffff;
    line-height: 10px;
}
#header .tags-suggest a:hover{
    color:#7eb0d8;
}
#header .tags-suggest a:last-child{
    border:none;
}

#header .cart{
    margin: 10px 0 0 10px;
    position:relative;
}
#header .cart .fa-shopping-cart{
    font-size:28px;
}
